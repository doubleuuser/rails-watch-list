<h1>create new bookmark</h1>

<%= simple_form_for @bookmark do |f| %>
  <%= f.association :movies, label: 'Choose which movie to bookmark' %>
  <%= f.association :lists, label: 'Choose which list to add to' %>
  <%= f.input_field :comment, as: :text, label: 'Add comment' %>
  <%= f.button :submit %>

<% end  %>
